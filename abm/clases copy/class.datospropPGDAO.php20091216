<?phpinclude_once("generic_class/class.PGDAO.php");/*DROP TABLE IF EXISTS `achaval`.`propiedad_caracteristicas`;CREATE TABLE  `achaval`.`propiedad_caracteristicas` (  `id_prop_carac` int(10) unsigned NOT NULL auto_increment,  `id_prop` int(10) unsigned NOT NULL,  `id_carac` int(10) unsigned NOT NULL,  `contenido` varchar(500) NOT NULL,  `comentario` varchar(500) NOT NULL,  PRIMARY KEY  (`id_prop_carac`)) ENGINE=InnoDB DEFAULT CHARSET=latin1;*/class DatospropPGDAO extends PGDAO {	protected $INSERT="INSERT INTO achaval.propiedad_caracteristicas (id_prop,id_carac,contenido,comentario) values (#id_prop#,#id_carac#,'#contenido#','#comentario#')";	protected $DELETE="DELETE FROM achaval.propiedad_caracteristicas WHERE id_prop=#id_prop#";	protected $UPDATE="UPDATE achaval.propiedad_caracteristicas SET id_prop=#id_prop#,id_carac=#id_carac#,contenido='#contenido#',comentario='#comentario#' WHERE id_prop_carac=#id_prop_carac# ";	protected $FINDBYID="SELECT id_prop_carac,id_prop,id_carac,contenido,comentario FROM achaval.propiedad_caracteristicas WHERE id_prop_carac=#id_prop_carac#";	protected $FINDBYCLAVE="SELECT id_prop_carac,id_prop,id_carac,contenido,comentario FROM achaval.propiedad_caracteristicas WHERE id_prop=#id_prop# and  id_carac=#id_carac#";		protected $COLECCION="SELECT p.id_prop_carac,p.id_prop,p.id_carac,p.contenido,p.comentario 							FROM achaval.propiedad_caracteristicas as p 							WHERE p.id_prop=#id_prop# and p.id_carac=#id_carac#";			} // Fin clase DAO?>